<view><loading class="vue-ref" vue-id="1" custom="{{false}}" shadeClick="{{false}}" type="{{2}}" data-ref="loading" bind:__l="__l"></loading><form data-event-opts="{{[['submit',[['submitScore',['$event']]]]]}}" bindsubmit="__e"><view class="cu-form-group"><view class="title">公寓ID</view><input name="apa_id" disabled="true" value="{{ScoreInfo.apa_id}}"/></view><view class="cu-form-group"><view class="title">公寓名称</view><input name="apa_name" disabled="true" value="{{ScoreInfo.apa_name}}"/></view><view class="cu-form-group"><view class="title">用户工号</view><input name="user_id" disabled="true" value="{{ScoreInfo.user_id}}"/></view><view class="cu-form-group margin-top"><view class="title">评分</view></view><block wx:if="{{starConfig.defaultStar>=0}}"><block><sunui-star class="sunui-star" vue-id="2" defaultStar="{{starConfig.defaultStar}}" maxStar="{{starConfig.maxStar}}" iconName="{{starConfig.iconName}}" starSize="{{starConfig.starSize}}" isTips="{{starConfig.isTips}}" disabledStar="{{starConfig.disabledStar}}" data-event-opts="{{[['^changeStar',[['changeStar']]]]}}" bind:changeStar="__e" bind:__l="__l"></sunui-star></block></block><block wx:if="{{modalName}}"><view class="cu-form-group"><view class="title">备注</view><textarea maxlength="255" placeholder="请输入评分原因" data-event-opts="{{[['input',[['textareaBInput',['$event']]]]]}}" bindinput="__e"></textarea></view></block><block wx:if="{{!modalName}}"><view class="cu-form-group"><view class="title">备注</view><text>{{ScoreInfo.remark}}</text></view></block><view class="padding"><button class="cu-btn round block bg-blue margin-tb-sm lg" disabled="{{!modalName}}" form-type="submit">{{btnText}}</button></view></form></view>